<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fittingChart">

    <application
        android:name=".database.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/salty_fish"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.SplashActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".ui.autoLogin.LoginActivity">
        </activity>
        <activity android:name="com.example.fittingChart.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.fittingChart.activity.FittingActivity">
        </activity>
    </application>


</manifest>